{
  "name": "flowscript",
  "version": "0.7.0",
  "description": "A TypeScript/JavaScript library designed to implement workflows as computational graphs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "test": "test"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "4.1.7",
    "@types/mocha": "5.2.6",
    "@types/node": "12.0.2",
    "chai": "4.2.0",
    "mocha": "6.1.4",
    "nyc": "14.1.1",
    "rimraf": "2.6.3",
    "ts-node": "8.1.0",
    "typescript": "3.4.5"
  },
  "keywords": [
    "workflow",
    "computation",
    "graph",
    "functional-programming",
    "fp",
    "railway-oriented-programming",
    "rop",
    "typescript",
    "javascript",
    "functor",
    "applicative",
    "monad",
    "monoid",
    "option",
    "result",
    "validation",
    "future"
  ],
  "scripts": {
    "clean": "rimraf dist/*",
    "transpile": "npm run clean && tsc -p tsconfig.production.json",
    "test": "mocha --require ts-node/register test/**/*.ts",
    "test:watch": "mocha --require ts-node/register test/**/*.ts --watch --watch-extensions ts",
    "coverage": "nyc mocha"
  },
  "author": "Sebastian E. Kwiatkowski",
  "license": "MIT",
  "nyc": {
    "all": true,
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "test/**/*.ts"
    ],
    "reporter": [
      "html", "text-summary"
    ],
    "temp-dir": "coverage/.nyc_output"
  }
}
