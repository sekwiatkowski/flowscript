{
  "name": "@sekwiatkowski/js-fp",
  "version": "0.18.1",
  "description": "A pragmatic functional programming library for JavaScript/TypeScript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "docs"
  ],
  "directories": {
    "test": "test"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "4.1.7",
    "@types/mocha": "5.2.6",
    "@types/node": "12.0.2",
    "chai": "4.2.0",
    "mocha": "6.1.4",
    "nyc": "14.1.1",
    "rimraf": "2.6.3",
    "ts-node": "8.1.0",
    "typescript": "3.5.2"
  },
  "keywords": [
    "functional-programming",
    "fp",
    "javascript",
    "typescript",
    "workflow",
    "flow",
    "computation",
    "graph",
    "railway-oriented-programming",
    "rop",
    "functor",
    "applicative",
    "monad",
    "option",
    "result",
    "validation",
    "future",
    "list",
    "state",
    "writer",
    "combination",
    "semigroup",
    "monoid",
    "predicate",
    "equivalence",
    "equality",
    "order"
  ],
  "scripts": {
    "clean": "rimraf dist/*",
    "build": "npm run clean && tsc -p tsconfig.production.json",
    "test": "mocha --require ts-node/register test/**/*.ts",
    "test:watch": "mocha --require ts-node/register test/**/*.ts --watch --watch-extensions ts",
    "coverage": "nyc mocha"
  },
  "author": "Sebastian E. Kwiatkowski",
  "license": "MIT",
  "repository": {
    "type" : "git",
    "url" : "https://github.com/sekwiatkowski/js-fp.git"
  },
  "nyc": {
    "all": true,
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "test/**/*.ts"
    ],
    "reporter": [
      "html",
      "text-summary"
    ],
    "temp-dir": "coverage/.nyc_output"
  }
}
